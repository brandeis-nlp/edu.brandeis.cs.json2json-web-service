{
  "name": "edu.brandeis.cs.json2json.Json2JsonWs",
  "version": "0.2.0",
  "description": "Json2Json Web Services",
  "vendor": "http://www.cs.brandeis.edu/",
  "allow": "http://vocab.lappsgrid.org/ns/allow#any",
  "license": "http://vocab.lappsgrid.org/ns/license#apache-2.0",
  "requires": {
    "metadata": {
      "op": [ "json2jsondsl","xml2jsondsl", "xml2xmlxsl", "xml2json", "json2xml" ],
      "template": "Json dsl template (op == json2jsondsl) / Xml dsl template (op == xml2jsondsl) / xslt template (op == xml2xmlxsl)",
      "default": "op = json2jsondsl"
    },
    "payload": { "sources": [ "source xml", "source json" ] }
  },
  "produces": { "language": [ "en" ] }
}